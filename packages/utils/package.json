{
  "name": "@millifx/utils",
  "version": "0.0.1",
  "author": "Jack Song",
  "license": "MIT",
  "description": "An utils package for MilliFX",
  "sideEffects": false,
  "main": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:package && yarn run build:types",
    "build:package": "tsc --project tsconfig.json",
    "build:types": "tsc --project tsconfig.types.json",
    "test": "jest test --watchAll=false",
    "format": "prettier --write src/**/*.ts{,x} --write",
    "lint": "tsc --noEmit --project tsconfig.json && eslint --fix"
  },
  "eslintConfig": {
    "extends": [
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!src/**/*.d.ts",
      "!src/index.tsx"
    ]
  },
  "dependencies": {
    "typescript": "^4.1.3"
  }
}
